#Options -Indexes		#zobrazovani souboru
ErrorDocument 400 /error_page/400.html
		#error page 400
#ErrorDocument 401 /error_page/401.html		#error page 401
#ErrorDocument 403 /error_page/403.html		#error page 403
ErrorDocument 404 /phplayout/error_page/404.html
		#error page 404
ErrorDocument 500 /error_page/500.html
		#error page 500
#ErrorDocument 503 /error_page/503.html		#error page 503
#AddHandler php5-cgi .php		#zapinani php5 na klenotu 1/2
#Action php5-cgi /php5cgi/php		#zapinani php5 na klenotu 2/2
		#http aturizace pro klenot server
RewriteEngine on		#zapinani rewrite


#RewriteRule ^zmena-jazyka-na-([a-zA-Z\_]+)/?$ ?sub=changelang&id=$1 [L]
#RewriteRule ^rss/([a-zA-Z-\_]+)/?$ ?action=rss&sablona=$1 [L]


RewriteRule ^autorizace/([0-9-]+)/?$ ?action=autorizace&id=$1 [L]
RewriteRule ^user([a-zA-Z-\_]+)/([0-9-]+)/?$ ?action=user$1&id=$2 [L]


RewriteRule ^galerie/([a-zA-Z-\_]+)/?$ ?action=galerie&sekce=$1 [L]
RewriteRule ^([a-zA-Z0-9-\_]+)/([0-9]+)?$ ?action=$1&str=$2 [L]
RewriteRule ^([a-zA-Z0-9-\_]+)/?$ ?action=$1 [L]

#RewriteRule ^([a-zA-Z0-9-\_]+)/serazeno-podle:([a-z\_]+)/pohled:([a-z\_]+)/strana:([0-9]+)/?$ ?action=$1&sort=$2&view=$3&str=$4 [L]
#RewriteRule ^([a-zA-Z0-9-\_]+)/([a-zA-Z0-9-\_]+)/?/?$ ?action=$1%%%%$2 [L]
#RewriteRule ^([a-zA-Z0-9-\_]+)/([a-zA-Z0-9-\_]+)/([a-zA-Z0-9-\_]+)/?/?/?$ ?action=$1%%%%$2%%%%$3 [L]
#RewriteRule ^([a-zA-Z0-9-\_]+)/([a-zA-Z0-9-\_]+)/([a-zA-Z0-9-\_]+)/([a-zA-Z0-9-\_]+)/?/?/?/?$ ?action=$1%%%%$2%%%%$3%%%%$4 [L]
#RewriteRule ^useredit/([0-9]+)/?$ ?action=useredit&id=$1 [L]
