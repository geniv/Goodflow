#zobrazovani souboru
Options -Indexes

#error page
#ErrorDocument 400 /error_page/index.php
#ErrorDocument 401 /error_page/index.php
#ErrorDocument 403 /error_page/index.php
#ErrorDocument 404 /error_page/index.php
#ErrorDocument 500 /error_page/index.php
#ErrorDocument 503 /error_page/index.php


ErrorDocument 400 /errors/400.html
ErrorDocument 401 /errors/401.html
ErrorDocument 402 /errors/402.html
ErrorDocument 403 /errors/403.html
ErrorDocument 404 /errors/404.html
ErrorDocument 500 /errors/500.html
ErrorDocument 501 /errors/501.html
ErrorDocument 502 /errors/502.html
ErrorDocument 503 /errors/503.html


#zapinani rewrite
RewriteEngine on

#nasmerovani korenu
RewriteBase /

#RewriteCond %{HTTP_HOST} ^gfdesign.cz
#RewriteRule (.*) http://www.gfdesign.cz/$1 [R=301,QSA,L]

RewriteRule ^([0-9]+)/?$ index.php?page=$1 [L]
RewriteRule ^([a-zA-Z-\_]+)/?$ index.php?action=$1 [L]
RewriteRule ^([a-zA-Z-\_]+)/([0-9]+)/?$ index.php?action=$1&page=$2 [L]
#RewriteRule ^([a-zA-Z0-9-\_]+)/([a-zA-Z-\_]+)/?$ index.php?action=$1&co=$2 [L]



